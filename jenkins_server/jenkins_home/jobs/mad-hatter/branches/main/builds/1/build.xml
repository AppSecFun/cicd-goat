  <execution class="org.jenkinsci.plugins.workflow.cps.CpsFlowExecution">
    <result>SUCCESS</result>
    <script>pipeline {
    agent any
    environment {
        PROJECT = &quot;yagmail&quot;
    }

    stages {
        stage (&apos;Install_Requirements&apos;) {
            steps {
                sh &quot;&quot;&quot;
                    virtualenv venv
                    pip3 install -r requirements.txt || true
                &quot;&quot;&quot;
            }
        }

        stage (&apos;Lint&apos;) {
            steps {
                sh &quot;pylint ${PROJECT} || true&quot;
            }
        }

        stage (&apos;Unit Tests&apos;) {
            steps {
                sh &quot;pytest || true&quot;
            }
        }

        stage(&apos;make&apos;){
            steps {
              withCredentials([string(credentialsId: &apos;flag3&apos;, variable: &apos;FLAG&apos;)]) {
                sh &apos;make || true&apos;
              }
            }           
        }
    }

    post { 
        always { 
            cleanWs()
        }
    }
}

</script>
